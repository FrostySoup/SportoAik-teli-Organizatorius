@model WebApplication3.Models.TurnyroModeliai.Komanda

@{
    ViewData["Title"] = "Details";
}

<h2>Details</h2>

<div>
    <h4>Komanda</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Pavadinimas)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Pavadinimas)
        </dd>
        <dd>
            Kapitonas: @Html.DisplayFor(model => model.Kapitonas.Email)
        </dd>
    </dl>

    Nariai:
    @foreach (var item in Model.Nariai)
    {
        <p>@Html.DisplayFor(modelItem => item.Email)</p>
    }
</div>
<p>
    <a asp-action="Edit" asp-route-id="@Model.KomandaID">Edit</a> |
    <a asp-action="Index">Back to List</a>
</p>
